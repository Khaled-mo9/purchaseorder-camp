<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
    <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
    <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Validation.V1.xml">
    <edmx:Include Alias="Validation" Namespace="Org.OData.Validation.V1"/>
  </edmx:Reference>
  <edmx:DataServices>
    <Schema Namespace="cdsView" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <Annotation Term="Core.Links">
        <Collection>
          <Record>
            <PropertyValue Property="rel" String="author"/>
            <PropertyValue Property="href" String="https://cap.cloud.sap"/>
          </Record>
        </Collection>
      </Annotation>
      <EntityContainer Name="EntityContainer">
        <EntitySet Name="viewAll" EntityType="cdsView.viewAll"/>
      </EntityContainer>
      <EntityType Name="viewAll">
        <Key>
          <PropertyRef Name="purchase_order_number"/>
        </Key>
        <Property Name="purchase_order_number" Type="Edm.Int64" Nullable="false"/>
        <Property Name="po_item" Type="Edm.Int32"/>
        <Property Name="delivery_date" Type="Edm.Date"/>
        <Property Name="material_number" Type="Edm.String" MaxLength="18"/>
        <Property Name="material_group" Type="Edm.String" MaxLength="9"/>
        <Property Name="language" Type="Edm.String" MaxLength="2"/>
        <Property Name="material_description" Type="Edm.String" MaxLength="400"/>
      </EntityType>
      <Annotations Target="cdsView.viewAll/purchase_order_number">
        <Annotation Term="Common.Label" String="{i18n>bp_poNum}"/>
      </Annotations>
      <Annotations Target="cdsView.viewAll/po_item">
        <Annotation Term="Common.Label" String="{i18n>bp_poItem}"/>
      </Annotations>
      <Annotations Target="cdsView.viewAll/delivery_date">
        <Annotation Term="Common.Label" String="{i18n>bp_deliveryDate}"/>
      </Annotations>
      <Annotations Target="cdsView.viewAll/material_number">
        <Annotation Term="Common.Label" String="{i18n>bp_materialNum}"/>
      </Annotations>
      <Annotations Target="cdsView.viewAll/material_group">
        <Annotation Term="Common.Label" String="{i18n>bp_materialGroup}"/>
      </Annotations>
      <Annotations Target="cdsView.viewAll/language">
        <Annotation Term="Common.Label" String="{i18n>bp_language}"/>
        <Annotation Term="Validation.AllowedValues">
          <Collection>
            <Record Type="Validation.AllowedValue">
              <Annotation Term="Core.SymbolicName" String="English"/>
              <PropertyValue Property="Value" String="EN"/>
            </Record>
            <Record Type="Validation.AllowedValue">
              <Annotation Term="Core.SymbolicName" String="Arabic"/>
              <PropertyValue Property="Value" String="AR"/>
            </Record>
          </Collection>
        </Annotation>
      </Annotations>
      <Annotations Target="cdsView.viewAll/material_description">
        <Annotation Term="Common.Label" String="{i18n>bp_materialDesc}"/>
      </Annotations>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>